{
	"name": "epic-utils",
	"displayName": "EpicUtils",
	"description": "VS Code Utils that makes your more productive.",
	"version": "1.0.4",
	"publisher": "MattRybin",
	"icon": "assets/logo.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/mattrybin/EpicUtils"
	},
	"keywords": [
		"utils",
		"code-review"
	],
	"engines": {
		"vscode": "^1.73.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:EpicUtils.changed-files-navigator"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "samples.quickInput",
				"title": "Quick Input Samples"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint \"src/**/*.ts\"",
		"watch": "tsc -watch -p ./"
	},
	"devDependencies": {
		"@types/node": "^16.11.7",
		"@types/vscode": "^1.73.0",
		"@typescript-eslint/eslint-plugin": "^5.42.0",
		"@typescript-eslint/parser": "^5.42.0",
		"eslint": "^8.26.0",
		"typescript": "^4.9.4"
	},
	"dependencies": {
		"@types/ramda": "^0.28.23",
		"ramda": "^0.28.0",
		"ramda-adjunct": "^3.4.0"
	}
}